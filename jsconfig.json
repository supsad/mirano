{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@utils/*": [
        "src/utils/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@modules/*": [
        "src/modules/*"
      ],
      "@scss/*": [
        "src/scss/*"
      ],
      "@/*": [
        "src/*"
      ]
    }
  }
}
